{
    "name": "henryaraujo/adielseffrinbot",
    "type": "project",
    "autoload": {
      "psr-4": {
        "AdielSeffrinBot\\": "src/"
      }
    },
    "require": {
      "react/event-loop": "^1.1",
      "react/filesystem": "^0.1.2",
      "wyrihaximus/react-child-process-pool": "^1.6",
      "phergie/phergie-irc-client-react": "^3.5",
      "phergie/phergie-irc-bot-react": "^2.0",
      "symfony/dotenv": "^5.2",
      "symfony/dependency-injection": "^5.2",
      "symfony/config": "^5.2",
      "symfony/yaml": "^5.2"
    },
    "scripts": {
      "start": "php index.php",
      "start:layer": "php -S 0.0.0.0:8080 public/flush.php",
      "server": "php server/server.php"
    },
    "config": {
      "process-timeout": 0
    }
}
