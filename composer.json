{
    "name": "henryaraujo/adielseffrinbot",
    "type": "project",
    "autoload": {
      "psr-4": {
        "AdielSeffrinBot\\": "src/"
      }
    },
    "require": {
      "php": "^7.0 || ^5.4 || ^8.0",
      "react/event-loop": "dev-master",
      "react/filesystem": "dev-master",
      "wyrihaximus/react-child-process-pool": "dev-master",
      "phergie/phergie-irc-client-react": "^3.1",
      "phergie/phergie-irc-bot-react": "^2.0",
      "vlucas/phpdotenv": "^5.3"
    },
    "scripts": {
      "start": "php index.php",
      "start:layer": "php -S 0.0.0.0:8080 layers/flush.php",
      "server": "php server/server.php"
    },
    "config": {
      "process-timeout": 0
    }
}
